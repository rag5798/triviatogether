<h1>Quiz Complete!</h1>
<p>Your score: <%= score %> out of <%= total %></p>

<% questions.forEach((q, i) => { %>
  <div class="question-review">
    <h3>Question <%= i + 1 %>: <%= q.questionText %></h3>
    <p>Your answer: 
      <strong class="<%= q.userAnswer === q.correctAnswer ? 'correct' : 'incorrect' %>">
        <%= q.userAnswer %>
      </strong>
    </p>
    <% if (q.userAnswer !== q.correctAnswer) { %>
      <p>Correct answer: <strong><%= q.correctAnswer %></strong></p>
    <% } %>
    <hr>
  </div>
<% }) %>